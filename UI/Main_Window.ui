/**/
QTabWidget::pane { /* The tab widget frame */
    border-top: 2px solid #077B8A;
    position: absolute;
    top: -0.5em;
}

QTabWidget::tab-bar {
    alignment: center;
}

/* Style the tab using the tab sub-control. Note that
    it reads QTabBar _not_ QTabWidget */
QTabBar::tab {
   /* 
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #A2D5C6, stop: 0.2 #077B8A,
                                stop: 0.8 #077B8A, stop: 1.0 #A2D5C6);
*/
	background-color:#A2D5C6;
    border: 2px solid #077B8A;
    border-bottom-color: #077B8A; /* same as the pane color */
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
    min-width: 8ex;
    padding: 5px;
	color:#077B8A;
}

QTabBar::tab:selected, QTabBar::tab:hover {
   /* 
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #077B8A, stop: 0.3 #A2D5C6,
                                stop: 0.7 #A2D5C6, stop: 1.0 #077B8A);
	*/
	background-color:#077B8A;
	color:#A2D5C6;

}

QTabBar::tab:selected {
    border-color: #077B8A;
    border-bottom-color: #077B8A; /* same as pane color */

}
